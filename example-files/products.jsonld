{
	"@context": {
		"name": "https://schema.org/name",
		"description": "https://schema.org/description",
		"sku": "https://schema.org/sku",
		"price": "https://schema.org/price",
		"category": "https://schema.org/category",
		"brand": "https://schema.org/brand",
		"inStock": "https://schema.org/availability",
		"stockQuantity": "https://example.com/vocab/stockQuantity",
		"rating": "https://schema.org/ratingValue",
		"reviews": "https://schema.org/reviewCount",
		"tags": "https://schema.org/keywords"
