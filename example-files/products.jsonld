	"ldc:schema": {
		"version": 1,
		"metadata": {
			"title": "Products",
			"description": "E-commerce product catalog with pricing and inventory."
		},
		"properties": [
			{
				"id": "prop:name",
				"key": "name",
				"title": "Product Name",
				"type": "title"
			},
			{
				"id": "prop:sku",
				"key": "sku",
				"title": "SKU",
				"type": "text"
			},
			{
				"id": "prop:description",
				"key": "description",
				"title": "Description",
				"type": "rich_text"
			},
			{
				"id": "prop:category",
				"key": "category",
				"title": "Category",
				"type": "select",
				"options": [
					{ "id": "opt:electronics", "name": "Electronics" },
					{ "id": "opt:clothing", "name": "Clothing" },
					{ "id": "opt:home", "name": "Home & Garden" },
					{ "id": "opt:sports", "name": "Sports" },
					{ "id": "opt:books", "name": "Books" }
				]
			},
			{
				"id": "prop:brand",
				"key": "brand",
				"title": "Brand",
				"type": "text"
			},
			{
				"id": "prop:price",
				"key": "price",
				"title": "Price",
				"type": "number"
			},
			{
				"id": "prop:in-stock",
				"key": "inStock",
				"title": "In Stock",
				"type": "checkbox"
			},
			{
				"id": "prop:stock-quantity",
				"key": "stockQuantity",
				"title": "Stock Qty",
				"type": "number"
			},
			{
				"id": "prop:rating",
				"key": "rating",
				"title": "Rating",
				"type": "number"
			},
			{
				"id": "prop:reviews",
				"key": "reviews",
				"title": "Reviews",
				"type": "number"
			},
			{
				"id": "prop:tags",
				"key": "tags",
				"title": "Tags",
				"type": "multi_select"
			},
			{
				"id": "prop:inventory-value",
				"key": "inventoryValue",
				"title": "Inventory Value",
				"type": "formula"
			}
		]
	},
