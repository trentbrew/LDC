{
  "@context": { "fn": "https://functions.test/" },
  "@id": "fn:builtins-test",

  "num": 16,
  "negNum": -42,
  "float": 3.14159,
  "str": "  Hello World  ",
  "date1": "2024-01-15",
  "date2": "2024-03-20",
  "arr": [5, 2, 8, 1, 9, 3],
  "nullVal": null,

  "test_sqrt": { "@expr": "$sqrt(num)" },
  "test_abs": { "@expr": "$abs(negNum)" },
  "test_round": { "@expr": "$round(float)" },
  "test_round_decimals": { "@expr": "$round(float, 2)" },
  "test_floor": { "@expr": "$floor(float)" },
  "test_ceil": { "@expr": "$ceil(float)" },
  "test_pow": { "@expr": "$pow(2, 10)" },
  "test_log10": { "@expr": "$log10(100)" },
  "test_pi": { "@expr": "$round($pi(), 4)" },

  "test_lower": { "@expr": "$lower(str)" },
  "test_upper": { "@expr": "$upper(str)" },
  "test_trim": { "@expr": "$trim(str)" },
  "test_len": { "@expr": "$len(str)" },
  "test_substr": { "@expr": "$substr(str, 2, 5)" },
  "test_replace": { "@expr": "$replace(str, 'World', 'LD-C')" },
  "test_startsWith": { "@expr": "$startsWith($trim(str), 'Hello')" },
  "test_endsWith": { "@expr": "$endsWith($trim(str), 'World')" },
  "test_contains": { "@expr": "$contains(str, 'World')" },
  "test_padStart": { "@expr": "$padStart('42', 5, '0')" },

  "test_today": { "@expr": "$today()" },
  "test_year": { "@expr": "$year(date1)" },
  "test_month": { "@expr": "$month(date1)" },
  "test_day": { "@expr": "$day(date1)" },
  "test_daysBetween": { "@expr": "$daysBetween(date1, date2)" },
  "test_formatDate": { "@expr": "$formatDate(date1, 'date')" },

  "test_if_true": { "@expr": "$if(num > 10, 'big', 'small')" },
  "test_if_false": { "@expr": "$if(num < 10, 'big', 'small')" },
  "test_default": { "@expr": "$default(nullVal, 'fallback')" },
  "test_coalesce": { "@expr": "$coalesce(nullVal, nullVal, 'found')" },
  "test_type_num": { "@expr": "$type(num)" },
  "test_type_str": { "@expr": "$type(str)" },
  "test_type_arr": { "@expr": "$type(arr)" },
  "test_isNull": { "@expr": "$isNull(nullVal)" },
  "test_isNumber": { "@expr": "$isNumber(num)" },
  "test_toNumber": { "@expr": "$toNumber('42')" },
  "test_toString": { "@expr": "$toString(num)" },

  "test_first": { "@expr": "$first(arr)" },
  "test_last": { "@expr": "$last(arr)" },
  "test_at": { "@expr": "$at(arr, 2)" },
  "test_count": { "@expr": "$count(arr)" },
  "test_sum_arr": { "@expr": "$sum(arr)" },
  "test_avg_arr": { "@expr": "$avg(arr)" },
  "test_min_arr": { "@expr": "$min(arr)" },
  "test_max_arr": { "@expr": "$max(arr)" },
  "test_reverse": { "@expr": "$reverse(arr)" },
  "test_unique": { "@expr": "$unique(arr)" },

  "test_chained": { "@expr": "$upper($trim(str))" },
  "test_complex": { "@expr": "'Result: ' + $toString($round($sqrt(num) * 100, 2))" }
}
